{
  "Name": "OrthancHeadless",
  "HttpServerEnabled": true,
  "HttpPort": 8042,
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": true,
  "RegisteredUsers": {
    "orthanc": "orthanc"
  },
  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "StowRS": true,
    "WadoRS": true,
    "QidoRS": true
  },
  "HttpServer": {
    "EnableCors": true 
  },
  "OrthancExplorer": {
    "Enable": false
  },
  "Plugins" : [
    "/usr/share/orthanc/plugins/libOrthancTranscoding.so"
  ],
  "Transcoding" : {
    "Enable": true,
    "AllowJpeg": true,
    "AllowJpeg2000": true,
    "DefaultTransferSyntax": "1.2.840.10008.1.2.4.90"
  }, 
  "StorageDirectory": "/var/lib/orthanc/db"
}
